/********* Structure *********/
html {
	padding:0;
	/* for W3C-compliant browsers */
	margin: 0;
}

body {
	height: 100%;
	padding: 40px 0 0;
	margin:0;
	background: rgba(76,76,76,1);
	background-image: linear-gradient(top, rgba(193,193,193,1) 0%, rgba(134,134,134,1) 12%, rgba(76,76,76,1) 54%);
	/* for Safari 5.03+ and Chrome 7+ */
	background-image: -webkit-gradient(linear, left top, left bottom, color-stop(0, rgba(193,193,193,1)), color-stop(0.1243055, rgba(134,134,134,1)), color-stop(0.53556920449405, rgba(76,76,76,1)));
	/* for Firefox 3.6+ */
	background-image: -moz-linear-gradient(top, rgba(193,193,193,1) 0%, rgba(134,134,134,1) 12%, rgba(76,76,76,1) 54%);
	/* for Opera 11.1+ */
	background-image: -o-linear-gradient(top, rgba(193,193,193,1) 0%, rgba(134,134,134,1) 12%, rgba(76,76,76,1) 54%);
	/* for IE */
	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#c1c1c1', endColorstr='#868686'endColorstr='#4c4c4c');
}

#container {
	height: 100%;
	width:90%;
	max-width:960px;
	clear:both;
	margin:0 auto;
}


#page {
	padding: 10px;
	background:white;
	width:936px;
	border: 2px solid #c1c1c1;
	height:100%;
}

.clearfix:after {
	content: ".";
	display: block;
	clear: both;
	visibility: hidden;
	line-height: 0;
	height: 0;
}
 
.clearfix {
	display: inline-block;
}

#ballot-box {
	max-width:100%;
	position:absolute;
	margin-top: -54px;
	left:12%;
}
 
/********* Fonts *********/

html, body, h1, h2, h3, h4 {
	font-family: 'LeagueGothicRegular', sans-serif;
	text-align:center;
}

p, div {
	font-family: 'Rokkitt', serif;
}

h1 {
	font-size:2em;
}

h2 {
	text-transform:uppercase;
	font-size:1.8em;
}
.sectionheading {
	text-transform:uppercase;
	text-align:center;
	font-size:2.75em;
	color: #D37A3C;
	padding:10px 0 10px 0;
	border-top:4px double #99C337;
	border-bottom:4px double #99C337;
}

.stars {
	font-size:.5em;
	vertical-align:middle;
	margin:0 10px;
}


/********* Header *********/

#header {
	text-align:right;
	margin-top:2%;
	margin-right:1.0416667%;
}

#logo {
	margin: -2px -302px 0 0;
}

#tagline {
	width:20.8333333%;
	margin-right:2.0833333%;
	float:right;
	clear:left;
}

/********* Main ***********/

#main {
	width:792px;
	margin-top:5%;
	margin-left: 16.6666667%;
	padding-bottom:100px;
	height:100%;
}

/******** Instructions *******/

#instructions-box {
	width: 206px;
	position:fixed;
	left:auto;
	top:10px;
	margin-left: -54px;
}
#instructions-box.top {
	top: 80px;
	position: absolute;
}
#instructions {
	background: #636363 url(<%= asset_path 'menu-bg.gif'%>) top left repeat;
	color:white;
	padding:0px;
	height:auto;
	text-align:left;
	font-size: 1.44em;
	border-color: transparent #000 #000;
	border-width: 1px;
	border-style: solid;
}
#instructions img.menutop {
	margin: -64px 0 0.72em -1px;
}
#instructions ul li:first-child a { border-top: none; }
#instructions ul li:last-child a { border-bottom: none; }
#instructions ul li a { 
	border-top: 1px solid #272727;
	border-bottom: 1px solid #706868;
}

#instructions ul {
	list-style-type:none;
	margin-top: -26px;
}

#instructions a, #instructions a:link, #instructions a:visited {
	display:block;
	padding:11px;
}
a, a:link, a:visited, a:active {
	color:white;
	cursor: pointer;
	text-decoration:none;
}
a:hover {
	text-decoration: underline;
}

#instructions a:hover {
	display:block;
	background-color:white;
	color:#636363;
}

#instructions li div.menu-inner {
	background: #dadada;
}
#instructions li div.menu-inner p:last-child, #instructions li div.menu-inner div:last-child { margin-bottom: 0;}
#instructions li div.menu-inner p, #instructions li div.menu-inner div {
	color: #303030;
	font-size: 15px;
	padding: 10px;
	margin-bottom: .6em;
}
#instructions li div.menu-inner .arrow { 
	margin: -10px auto -13px;
	height: 0;
	width: 0;
	border-color: #303030 transparent transparent;
	border-style: solid;
	border-width: 8px;
	padding: 0;
}
#instructions li:hover div.menu-inner .arrow {
	border-color: #fff transparent transparent;
}
#instructions li div.container {
	padding: 12px;
}
#instructions li a.small {
	border: none;
	font-size: 14px;
	padding: 0;
	line-height: 16px;
}
.share-container {
	border-bottom: 1px solid #706868;
}

/******** Ballot Sections *******/

.ballot-section {
	width: 720px;
	margin: 20px auto;
}

/********* Forms ************/

input {
	border-top: none;
	border-left:none;
	border-right:none;
	width:55%;
	font-family:'Waiting for the sunrise', 'Rokkit', serif;
	color:#99C337;
	font-size:1.4em;
}

input:focus {
	outline:none;
}	

button {
	background-color:#99C337;
	outline:none;
	border:none;
	font-family:"Rokkitt", serif;
	text-transform:uppercase;
	font-size:1.7em;
	color:white;
	width:85%;
	
}
	
/******* Find Ballot **********/
#find-ballot {
	height:auto;
}

.find-ballot:focus {
	padding: 19px 10px 14px;
}
.find-ballot {
	width:500px;
 	margin: 25px auto 15px;
	-webkit-appearance:none;
	border-radius:4px 0px 0px 4px;
	font-family:'Rokkitt', serif;
	font-size:1.45em;
	padding: 19px 10px 15px;
	background-color:#ceea8e;
	box-shadow:inset 0px 1px 1px 0px rgba(0,0,0,.3);
	border:none;
	color:#fff;
	text-shadow:0px 1px 1px #999;
	outline:none;
	box-sizing: border-box;
	-moz-box-sizing: border-box;
}


input.find-ballot::-webkit-input-placeholder {
    color:#8eb636;
}
input.find-ballot:-moz-placeholder {
    color:#8eb636;
}

a.find-ballot-submit {
	background:-webkit-gradient( linear, left top, left bottom, color-stop(0.05, #ceea8e), color-stop(1, #8db237) );
	background:-moz-linear-gradient( center top, #ceea8e 5%, #8db237 100% );
	filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#ceea8e', endColorstr='#8db237');
	background-color:#ceea8e;
	-moz-border-radius:0px 2px 2px 0px;
	-webkit-border-radius:0px 2px 2px 0px;
	border-radius:0px 2px 2px 0px;
	border:1px solid #8db237;
	display:inline-block;
	color:#fff;
	font-family:'Rokkitt', serif;
	font-size:20px;
	font-weight:bold;
	padding:20px 10px 16px 10px;
	text-decoration:none;
	margin:0px 0px 0px 0;
	width:80px;
	text-align:center;
	text-shadow: 0px -1px 1px #637c2a;
	filter: dropshadow(color=#637c2a, offx=0, offy=-1);
	text-transform:uppercase;
}

.find-ballot-submit:hover {
	text-decoration:underline;
}


.find-ballot-submit:active {
	background:-webkit-gradient( linear, left top, left bottom, color-stop(0.05, #8db237), color-stop(1, #ceea8e) );
	background:-moz-linear-gradient( center top, #8db237 5%, #ceea8e 100% );
	filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#8db237', endColorstr='#ceea8e');
	background-color:#8db237;
	box-shadow: inset 0px 1px 1px 1px #7a9931;
	-moz-box-shadow: inset 0px 1px 1px 1px #7a9931;
	-webkit-box-shadow: inset 0px 1px 1px 1px #7a9931;
}


#map .wrapper {
	background: #8eb237;
	width: 500px;
	margin: 0 auto;
	padding: 0;
	line-height: 0;
	z-index: 1;
	position: relative;
}
#map .confirmation {
	font-size: 60px;
	color: #fff;
	text-shadow: 0px 1px 4px #222;
	margin: -150px 0 0;
	z-index: 2;
	position: relative;
	height: 150px;
}
#map .confirmation a {
	display: inline-block;
}
#map .confirmation a:hover {
	color: #D37A3C;
}

#map img {
	width: 500px;
	margin: 0 auto;
	border:1px solid #99C337;
	opacity: .6;
}	




.generate {
	width:220px;
	margin:15px auto 30px;
	text-transform:uppercase;
}

/********** Read Ballot ********/

.ballot-category {
	width: 100%;
	height:auto;
	margin-bottom:5%;
}

.category-heading {
	margin:-2.5% auto;
	background:white;
	width:30%;
	padding:1%;
}

.race-menu {
	width: 24.2424242%; /* 160px / 660px */
	float:right;
	margin:2.5% 3.030303%;
}

.race-menu ul {list-style-type:none}

.race-menu li {
	display:block;
	background-color: #C2DC84;
	text-align:center;
	margin-bottom:10%;
	color:white;
	font-size:1.5em;
	padding:4%;
}

	/******** Generic Choice ********/
	.row, .row.complete.selected {
		width: 680px;
		margin: 2.5% auto;
		padding: 0;
		border: #945931 1px solid;
		background: #d37a3c url(<%= asset_path 'ballot-bg.gif'%>) top left repeat;
	}
	.row.complete {
		border: #000 1px solid;
		background: #555 url(<%= asset_path 'ballot-finished-bg.gif'%>) top left repeat;
	}
	.row.commentable h1.title::before {
		content: " ";
        border: 4px solid #fff;
        height: 16px;
        width: 16px;
        border-radius: 20px;
        background: #fff;
    	margin: 0 0 0 -45px;
    	text-indent: -1px;
    	line-height: .7em;
		color: #d37a3c;
		font-size: 30px;
		position: absolute;
	}
	.row.complete h1.title::before {
		content: "\2713";
	}
	.row.commentable h1.title {
		text-indent: 60px;
	}


	.row h1.title {
		font-family: Rokkitt, serif;
		padding: 10px;
		text-align: left;
		text-transform: uppercase;
	}
	.row .counter > *, .row h1.title {
		-webkit-touch-callout: none;
		-webkit-user-select: none;
		-khtml-user-select: none;
		-moz-user-select: none;
		color: #fff;
		-ms-user-select: none;
		user-select: none;
		cursor: pointer;
	}
	.row.selected h1.title, .row.selected h1.counter { cursor: default; }
	.row h1.title:hover, .row.selected h1.title { 
		background: #d7874f url(<%= asset_path 'ballot-active-bg.gif'%>) top left repeat-x;
	}
	.row.finsihed h1.title:hover, .row.finsihed h1.title { background: #555 url(<%= asset_path 'ballot-finished-bg.gif'%>) top left repeat; }
	.row .counter div {
		display: inline-block;
	}

	.row .counter span {
		font-size: 16px;
		pointer-events: none;
	}
	.row .counter {
		margin: .8em 40px 0 0;
		font-size: 20px;
		font-weight: bold;
		color: #fff;
		pointer-events: none;
		float: right;
	}

	.explanation {
		width:90%;
		padding:3%;
		color: #945931;
		text-align: left;
		margin: 0 0 10px;
	}
	.body {

		display: none;
	}
	.body .option:first-child {
		border-right: #e5a87f 1px solid;
		margin-left: 34px;
		padding-left: 8px;
		padding-right: 20px;
		width: 49%;
	}
	.body .optionholder {
		position: relative;
		width: 600px;
	}

	.option {
		width: 44%;
		box-sizing: border-box;
        -moz-box-sizing: border-box;
		padding: 0 0 60px 34px;
		float:left;
	}
	.option img {
		float:left;
		margin: 0 4% 4% 0;
		max-width: 36.6666667%;
	}
	.option div.blurb {
		color:white;
		text-align:left;
		margin: 0 0 10px;
		}
	.blurb.readMored {
		min-height: 220px;
		}
	.option h1 {
		font-family:'Rokkitt', serif;
		color:white;
		font-size:1.2em;
		line-height: 1.4em;
		float:none;
		width:auto;
		text-align: left;
		font-weight: bold;
		}
	.minicounter::after {
			content: '';
			position: absolute;
			width: 0;
			height: 0;
			border-color: transparent transparent transparent #f4bc94;
			border-width: 18px;
			border-style: solid;
			margin: -36px 0 0 133px;
	}
		.minicounter::before {
			content: '';
			position: absolute;
			width: 0;
			height: 0;
			margin-left: 130px;
			border-color: transparent transparent transparent #e6aa81;
			border-width: 18px;
			margin: -4px 0 0 134px;
			border-style: solid;
		}
	.minicounter div {
		width: 120px;
		overflow: hidden;
		position: relative;
	}
	.minicounter {
		float: left;
		position: relative;
		line-height: 1.2em;
	    overflow: visible;
	    height: 28px;
		font-size: 14px;
		text-align: left;
		border: 1px solid #e6aa81;
		padding: 3px 4px;
		margin: 4px 18px 10px 0;
		width: 130px;
		background-color: #f4bc94;
		color: white;
	}
	img.miniimage {
		width: 28px;
		height: 28px;
		float: left;
		margin: 8px 0 0 2px;
	}
	.faces {
		height: 60px;
		position: absolute;
		bottom: 0;
		width: 100%;
	}
	.comment, .comment:focus {
		width:350px;
		height:115px;
		-webkit-appearance:none;
		font-family:'PT Sans', sans-serif;
		color:#d77a3c;
		font-size:16px;
		vertical-align:top;
		border:1px solid #d77a3c;
		box-shadow:inset 0px 0px 4px 1px #ccc;
		border-radius:2px;
		display:block;
		resize: none;
		outline:none;
		padding:5px;
	}
	body.not_logged_in .yourFeedback:hover .comment:disabled::-webkit-input-placeholder,
	body.not_logged_in .yourFeedback:hover.comment:disabled::-moz-input-placeholder,
	body.not_logged_in .yourFeedback:hover .comment:disabled::placeholder {
		text-decoration: underline;
		font-weight: bold;
	}
	body.not_logged_in .yourFeedback:hover, body.not_logged_in .yourFeedback:hover .comment {
		cursor: pointer;
	}
	.yourFeedback {
        width: 590px;
		text-align: right;
		clear: both;
		border-top: #e5a87f 1px solid;
		border-bottom: #e5a87f 1px solid;
		margin: 0 34px;
		padding: 20px 0 10px;
	}
	.yourFeedback .head img {
		width: 50px;
		height: 50px;
	}
	.yourFeedback .head {
	    padding: 20px 0 10px;
		width: 50px;
		margin: 0 auto;
	}
	.yourFeedback h1, .yourFeedback h3 {
		text-align: center;
		font-family:'Rokkitt', serif;
		text-shadow: 0 2px 2px #fff;
	}
	.yourFeedback .buttons {
		float:left; 
		margin:14px 12px 0 10px;
		width: 196px;
	}
	.yourFeedback .buttons p {
		text-align: left;
		margin: -10px 0 10px
	}
	

	.link {
		cursor: pointer;
		font-size: 90%;
		color: #D37A3C;
	}
	.link:hover {
		text-decoration: underline;
	}

	.race-menu ul li {
		cursor: pointer;
		opacity: 0.5;
	}
	.race-menu ul li:hover {
		opacity: 0.2;
	}
	.race-menu ul li.selected, .race-menu ul li.selected:hover { opacity: 1 }
	


	/******** Ballot Measures ********/
	.ballot-measures { color:#D37A3C }
	.ballot-measures .row { background-color:#e4bc9e }


	.body div.commentTitle {
		text-align: left;
		text-transform: uppercase;
		font-size: 130%;
		margin: 10px 45px;
	}
	.body div.commentTitle .helper a:hover { text-decoration: underline }
	.body div.commentTitle .helper {
		margin-top: 10px;
		font-size: 16px;
		text-align: right;
		color: #fff;
	}
	.body div.commentTitle select {
		float: right;
	}
	
	ul.feedback li {  margin: 10px 60px; text-align: left; clear: both }
	ul.feedback li a:hover { text-decoration: underline; }

	ul.feedback li .head { width: 60px; display: inline-block }
	ul.feedback li img { max-height: 50px; width: 50px; }


	.head.yes .overlayBg, .selected ul.feedback li.yes .overlayBg { position:absolute; height: 50px; width: 50px; opacity: .8; background-color: #8eb337 }
	.head.no .overlayBg, ul.feedback li.no .overlayBg { position:absolute; height: 50px; width: 50px; opacity: .8; background-color: #bd393d }
	.head.yes .overlayText, ul.feedback li.yes .overlayText { position:absolute; height: 50px; width: 50px; opacity: .8; color: #fff; font-size: 24px; text-transform: uppercase; margin: 16px 4px; font-weight: bold; text-align: left  }
	.head.no .overlayText, ul.feedback li.no .overlayText { position:absolute; height: 50px; width: 50px; opacity: .8; color: #fff; font-size: 24px; text-transform: uppercase; margin: 16px 8px; font-weight: bold; text-align: left  }
	
	
	ul.feedback li .message { width: 480px; height: auto; display: inline-block; vertical-align: top; color: #464646; padding: 4px 0; }

	ul.feedback li .foot { float: right; text-align: right; width: 300px; font-size: 90%; color: #fff; }
	ul.feedback li .foot .ask { padding-right: 4px }
	a.full:link, a.full:active, a.full:visited, a.full:hover { text-align: right; color: #aaa }
	.footer {
		padding: 30px 60px;
		text-align: center;
	}
	
	

	/********* Support Button *********/

	.buttons .yes {
		text-transform: uppercase;
		background:-webkit-gradient( linear, left top, left bottom, color-stop(0.05, #ceea8e), color-stop(1, #8db237) );
		background:-moz-linear-gradient( center top, #ceea8e 5%, #8db237 100% );
		filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#ceea8e', endColorstr='#8db237');
		background-color:#ceea8e;
		-moz-border-radius:2px;
		-webkit-border-radius:2px;
		border-radius:2px 0px 0px 2px;
		border:1px solid #8db237;
		display:inline-block;
		color:#fff;
		font-family:'Rokkitt', serif;
		font-size:15px;
		font-weight:bold;
		padding:10px;
		text-decoration:none;
		margin-right:0;
		float: left;
		width:74px;
		text-align:center;
		text-shadow: 0px -1px 1px #637c2a;
		filter: dropshadow(color=#637c2a, offx=0, offy=-1);
	}

	.buttons .yes:hover, .yes.picked {
		background:-webkit-gradient( linear, left top, left bottom, color-stop(0.05, #8db237), color-stop(1, #ceea8e) );
		background:-moz-linear-gradient( center top, #8db237 5%, #ceea8e 100% );
		filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#8db237', endColorstr='#ceea8e');
		background-color:#8db237;
		box-shadow: inset 0px 1px 1px 1px #7a9931;
		-moz-box-shadow: inset 0px 1px 1px 1px #7a9931;
		-webkit-box-shadow: inset 0px 1px 1px 1px #7a9931;
	}

	.buttons .yes:active {
		position:relative;
		top:1px;
	}

	body.not_logged_in .yourFeedback .buttons .yes, body.not_logged_in .yourFeedback .buttons .yes:hover {
